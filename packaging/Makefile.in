@WITH_WIN32_TRUE@nsis: sndstud-$(VERSION)-setup.exe
@WITH_WIN32_TRUE@msi: sndstud-$(VERSION).msi

@WITH_WIN32_TRUE@2.12.10-dist: gtk+-bundle-2.12.10.zip
@WITH_WIN32_TRUE@	mkdir $@
@WITH_WIN32_TRUE@	-cd $@ && unzip -q ../gtk+-bundle-2.12.10.zip \
@WITH_WIN32_TRUE@	  `cat ../2.12.10-dist.txt`
@WITH_WIN32_TRUE@	echo 'gtk-theme-name = "MS-Windows"' > \
@WITH_WIN32_TRUE@	  2.12.10-dist/etc/gtk-2.0/gtkrc

@WITH_WIN32_TRUE@instdir:
@WITH_WIN32_TRUE@	tmpinst=`pwd`/instdir && \
@WITH_WIN32_TRUE@	  cd $(top_builddir) && $(MAKE) install prefix=$$tmpinst
@WITH_WIN32_TRUE@	if [ -d instdir/lib ]; then \
@WITH_WIN32_TRUE@	  mv instdir/lib/locale instdir/share; \
@WITH_WIN32_TRUE@	  rmdir instdir/lib; \
@WITH_WIN32_TRUE@	fi

@WITH_WIN32_TRUE@sndstud-$(VERSION)-setup.exe: sndstud.nsi english.nsh spanish.nsh \
@WITH_WIN32_TRUE@  license.rtf instdir 2.12.10-dist
@WITH_WIN32_TRUE@	if [ -n "$(TEST_INST)" ]; then \
@WITH_WIN32_TRUE@	  makensis -DVERSION=$(VERSION) -DTEST_INST sndstud.nsi; \
@WITH_WIN32_TRUE@	else \
@WITH_WIN32_TRUE@	  makensis -DVERSION=$(VERSION) sndstud.nsi; \
@WITH_WIN32_TRUE@	fi

@WITH_WIN32_TRUE@ls-r.txt: 2.12.10-dist
@WITH_WIN32_TRUE@	ls -RF $< | \
@WITH_WIN32_TRUE@	  sed -e '/^.*\/$$/d' -e 's/\(^.*\)\*/\1/g' > $@

@WITH_WIN32_TRUE@ls-r2.txt: instdir
@WITH_WIN32_TRUE@	ls -RF $< | \
@WITH_WIN32_TRUE@	  sed -e '/^.*\/$$/d' -e 's/\(^.*\)\*/\1/g' > $@

@WITH_WIN32_TRUE@uuids.txt:
@WITH_WIN32_TRUE@	uuidgen -c -n1000 > uuids.txt

@WITH_WIN32_TRUE@File.idt Directory.idt Component.idt Feature.idt \
@WITH_WIN32_TRUE@FeatureComponents.idt Media.idt: features.txt uuids.txt ls-r.txt ls-r2.txt
@WITH_WIN32_TRUE@	msi-tool -r -d"sndstud|Sound Studio" ls-r.txt ls-r2.txt

@WITH_WIN32_TRUE@archive.cab: File.idt
@WITH_WIN32_TRUE@	if [ -n "$(TEST_INST)" ]; then \
@WITH_WIN32_TRUE@	  cd 2.12.10-dist && cabarc n archive.cab @cablist.txt; \
@WITH_WIN32_TRUE@	else \
@WITH_WIN32_TRUE@	  cd 2.12.10-dist && cabarc -m LZX:21 n archive.cab @cablist.txt; \
@WITH_WIN32_TRUE@	fi
@WITH_WIN32_TRUE@	mv 2.12.10-dist/archive.cab .
@WITH_WIN32_TRUE@	rm -rf 2.12.10-dist instdir

@WITH_WIN32_TRUE@sndstud-$(VERSION).msi: Property.idt File.idt Directory.idt Component.idt \
@WITH_WIN32_TRUE@  Feature.idt FeatureComponents.idt Media.idt archive.cab Schema.msi
# Prepare tables that the installer will use to create shortcuts.
@WITH_WIN32_TRUE@	mv Directory.idt Directory.idt.old
@WITH_WIN32_TRUE@	cat Directory.idt.old Directory.idt.cat > Directory.idt
@WITH_WIN32_TRUE@	mkdir Icon
@WITH_WIN32_TRUE@	cp ../svgs/sndstud.ico Icon/sndstud_icon.exe.ibd
# Note: The component manipulation code is an inelegant solution to
# get the shortcut to the user manual to work.
@WITH_WIN32_TRUE@	mv Component.idt Component.idt.old
@WITH_WIN32_TRUE@	SSCOMP=`grep -F 'sndstud.exe' File.idt | cut -f2`; \
@WITH_WIN32_TRUE@	  HELPNAME=`grep -F 'help.txt' File.idt | cut -f1`; \
@WITH_WIN32_TRUE@	  HELPCOMP=`grep -F 'help.txt' File.idt | cut -f2`; \
@WITH_WIN32_TRUE@	  HCLINENUM=`grep -n -F $$HELPCOMP Component.idt.old | cut -d: -f1`; \
@WITH_WIN32_TRUE@	  HCOLD=`sed -ne $${HCLINENUM}p Component.idt.old`; \
@WITH_WIN32_TRUE@	  sed -e $${HCLINENUM}d Component.idt.old > Component.idt; \
@WITH_WIN32_TRUE@	  echo "`echo \"$$HCOLD\" | cut -f1-5`"$$'\t'$$HELPNAME >> \
@WITH_WIN32_TRUE@	    Component.idt; \
@WITH_WIN32_TRUE@	  sed -e s/SndStudComp/$${SSCOMP}/g -e s/SndStudFeat/ft1/g \
@WITH_WIN32_TRUE@	    -e s/HelpComp/$${HELPCOMP}/g \
@WITH_WIN32_TRUE@	    Shortcut.idt.in > Shortcut.idt; \
@WITH_WIN32_TRUE@	  sed -e s/SndStudComp/$${SSCOMP}/g RemoveFile.idt.in > \
@WITH_WIN32_TRUE@	    RemoveFile.idt
# Build the installer.
@WITH_WIN32_TRUE@	cp Schema.msi $@
@WITH_WIN32_TRUE@	msiinfo $@ -t "Installation Database" -j "Sound Studio" \
@WITH_WIN32_TRUE@	  -a "Andrew Makousky" -k "Installer, MSI, Database" \
@WITH_WIN32_TRUE@	  -o "This installer database contains the logic and data required to install Sound Studio." \
@WITH_WIN32_TRUE@	  -p "Intel;1033" -v {`uuidgen -c`} -g 110 -w 2 -u 0
@WITH_WIN32_TRUE@	msidb -d$@ -f`pwd | sed -e 's.^\(\|/cygdrive\)/\([[:alpha:]]\)/.\2:/.g' -e 's./.\\\\.g'` \
@WITH_WIN32_TRUE@	  -i Property.idt Component.idt Directory.idt \
@WITH_WIN32_TRUE@	  Feature.idt FeatureComponents.idt File.idt Media.idt \
@WITH_WIN32_TRUE@	  Icon.idt Shortcut.idt RemoveFile.idt
@WITH_WIN32_TRUE@	msidb -d$@ -aarchive.cab
@WITH_WIN32_TRUE@	! msival2 $@ "C:\\Program Files\\MsiVal2\\darice.cub" | grep -e 'ERROR'

@WITH_WIN32_TRUE@mostlyclean: mostlyclean-am
@WITH_WIN32_TRUE@	rm -rf instdir 2.12.10-dist Icon

@WITH_WIN32_TRUE@clean: clean-am mostlyclean
